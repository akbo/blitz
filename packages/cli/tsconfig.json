{
  "extends": "../../tsconfig.json",
  "include": ["src", "types"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "target": "es2015",
    "baseUrl": "./",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "lib",
    "declaration": false,
    "sourceMap": false,
    "noEmit": false,
    "incremental": true,
    "tsBuildInfoFile": ".tsbuildinfo"
  }
}
